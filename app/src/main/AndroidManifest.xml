            android:name="com.jsyh.xjd.activity.me.CreateOrderActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|stateUnchanged"/>
        <activity
            android:name="com.jsyh.xjd.activity.me.SelectAreaActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.jsyh.xjd.activity.me.AboutActivity"
            android:screenOrientation="portrait"/>
        <activity
