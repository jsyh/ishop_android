            android:screenOrientation="portrait"/>
        <activity
            android:name="com.jsyh.xjd.activity.me.MeAccountActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.jsyh.xjd.activity.me.SetupActicity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.jsyh.xjd.activity.me.ApplyServiceActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.jsyh.xjd.activity.me.CommentActivity"
            android:screenOrientation="portrait"/>
        <activity
